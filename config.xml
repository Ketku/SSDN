    id        = "com.ssdn.mba"
    version   = "2.0.0">
    <name>Su Socio de Negocios</name>
    
    <description>
        Su Socio de Negocios
    </description>
    
    <author href="http://susociodenegocios.com" email="pketonen@elclasificado.com">
        Apache Cordova Team
    </author>
    
    <icon src="res/icon/icon.png"        width="512" height="512" />
    
